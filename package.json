{
  "name": "daily-bytes",
  "version": "1.0.0",
  "description": "Abdelrhman Safwat solutions for Daily Bytes problems",
  "scripts": {
    "test": "npm test",
    "lint:eslint": "eslint 'src/**/*.js'",
    "format": "prettier src/* --write",
    "update-coc": "weallbehave -o .",
    "update-contrib": "weallcontribute -o .",
    "release": "standard-version",
    "publish": "git push --follow-tags origin master",
    "commit": "git-cz",
    "commit:retry": "git-cz --retry",
    "commitmsg": "commitlint -e"
  },
  "keywords": [
    "daily-bytes",
    "problems",
    "data-structures",
    "algorithms"
  ],
  "author": "Abdelrhman Safwat",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "commitizen": "^4.2.1",
    "commitlint": "^11.0.0",
    "cz-customizable": "^6.3.0",
    "prettier": "^2.1.2",
    "standard-version": "^9.0.0",
    "weallbehave": "^1.2.0",
    "weallcontribute": "^1.0.9"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "cz-customizable.config.js"
    }
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SonOfSafwat/daily-bytes.git"
  },
  "bugs": {
    "url": "https://github.com/SonOfSafwat/daily-bytes/issues"
  },
  "homepage": "https://github.com/SonOfSafwat/daily-bytes#readme"
}
